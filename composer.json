{
    "name": "slait/restfulapi",
    "description": "A package for performing CRUD processes in restful API",
    "license": "MIT",
    "authors": [
        {
            "name": "Lubem Tser",
            "email": "enginlubem@ymail.com",
            "homepage": "http://slait.com.ng"
        }
    ],
    "homepage": "https://github.com/slait/restfulapi",
    "keywords": ["Laravel", "RestfulApi"],
    "require": {
        "illuminate/support": "~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Slait\\RestfulApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slait\\RestfulApi\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slait\\RestfulApi\\RestfulApiServiceProvider"
            ],
            "aliases": {
                "RestfulApi": "Slait\\RestfulApi\\Facades\\RestfulApi"
            }
        }
    }
}
